<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="Main">
        <Setter Property="StackPanel.HorizontalAlignment" Value="Center" />
        <Setter Property="StackPanel.VerticalAlignment" Value="Center" />
        <Setter Property="StackPanel.Width" Value="795" />
        <Setter Property="StackPanel.Height" Value="1128" />
    </Style>

    <Style x:Key="Main1">
        <Setter Property="StackPanel.HorizontalAlignment" Value="Center" />
        <Setter Property="StackPanel.VerticalAlignment" Value="Center"/>
        <Setter Property="StackPanel.IsHitTestVisible" Value="False"/>
    </Style>

    <Style x:Key="Main2">
        <Setter Property="StackPanel.Clip">
            <Setter.Value>
                <RectangleGeometry Rect="0,0,655,263" RadiusX="36" RadiusY="36"/>
            </Setter.Value>
        </Setter>
        <Setter Property="StackPanel.Orientation" Value="Horizontal" />
        <Setter Property="StackPanel.Margin" Value="0 95 0 0"/>
        <Setter Property="StackPanel.Background" Value="#33FFFFFF"/>
        <Setter Property="StackPanel.Height" Value="263"/>
        <Setter Property="StackPanel.Width" Value="655"/>
    </Style>

    <Style x:Key="Main3">
        <Setter Property="StackPanel.Orientation" Value="Horizontal"/>
        <Setter Property="StackPanel.Width" Value="607" />
        <Setter Property="StackPanel.Height" Value="215" />
        <Setter Property="StackPanel.HorizontalAlignment" Value="Center"/>
        <Setter Property="StackPanel.VerticalAlignment" Value="Center"/>
        <Setter Property="StackPanel.Margin" Value="24" />
    </Style>

    <Style x:Key="MainStackPanel1">
        <Setter Property="StackPanel.Orientation" Value="Vertical" />
        <Setter Property="StackPanel.VerticalAlignment" Value="Top"/>
        <Setter Property="StackPanel.Width" Value="960"/>
        <Setter Property="StackPanel.Margin" Value="60 169 60 0"/>
    </Style>

    <Style x:Key="MainStackPanel11">
        <Setter Property="StackPanel.Margin" Value="0 95 0 0"/>
        <Setter Property="StackPanel.Clip">
            <Setter.Value>
                <RectangleGeometry Rect="0,0,960,1158" RadiusX="36" RadiusY="36"/>
            </Setter.Value>
        </Setter>
        <Setter Property="StackPanel.Orientation" Value="Vertical" />
        <Setter Property="StackPanel.HorizontalAlignment" Value="Right"/>
        <Setter Property="StackPanel.VerticalAlignment" Value="Center"/>
        <Setter Property="StackPanel.Margin" Value="0 42 0 0"/>
        <Setter Property="StackPanel.Background" Value="#33FFFFFF"/>
        <Setter Property="StackPanel.Height" Value="1158"/>
        <Setter Property="StackPanel.Width" Value="960"/>
    </Style>

    <Style x:Key="GridTestSberQ">
        <Setter Property="Grid.Margin" Value="0 8 0 0" />
    </Style>

    <Style x:Key="MainStackPanel_Result">
        <Setter Property="StackPanel.HorizontalAlignment" Value="Center" />
        <Setter Property="StackPanel.VerticalAlignment" Value="Top"/>
        <Setter Property="StackPanel.Margin" Value="142 169 142 0" />
    </Style>

    <Style x:Key="MainStackPanel2_Result" BasedOn="{StaticResource MainStackPanel_Result}">
        <Setter Property="StackPanel.Margin" Value="0 73 0 0" />
    </Style>

    <Style x:Key="Background_Popap">
        <Setter Property="StackPanel.Width" Value="1080" />
        <Setter Property="StackPanel.Height" Value="1920" />
        <Setter Property="StackPanel.Background" Value="#cc002436" />
    </Style>

    <Style x:Key="ModalWindow_Policy">
        <Setter Property="StackPanel.Width" Value="960" />
        <Setter Property="StackPanel.Height" Value="1800" />
        <Setter Property="StackPanel.Background" Value="White" />
        <Setter Property="StackPanel.Margin" Value="0 60 0 0" />
        <Setter Property="StackPanel.Clip">
            <Setter.Value>
                <RectangleGeometry RadiusX="36" RadiusY="36" Rect="0,0,960,1800" />
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="stackpanel1">
        <Setter Property="StackPanel.Orientation" Value="Horizontal" />
        <Setter Property="StackPanel.HorizontalAlignment" Value="Center" />
        <Setter Property="StackPanel.VerticalAlignment" Value="Center" />
    </Style>

    <Style x:Key="ModalWindow_Agreement" BasedOn="{StaticResource ModalWindow_Policy}">
        <Setter Property="StackPanel.Height" Value="1122" />
        <Setter Property="StackPanel.Margin" Value="0 399 0 0" />
        <Setter Property="StackPanel.Clip">
            <Setter.Value>
                <RectangleGeometry RadiusX="36" RadiusY="36" Rect="0,0,960,1122" />
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="ModalWindow_Error">
        <Setter Property="StackPanel.Width" Value="960" />
        <Setter Property="StackPanel.Height" Value="713" />
        <Setter Property="StackPanel.Background">
            <Setter.Value>
                <ImageBrush ImageSource="pack://application:,,,/SberQ;component/Assets/Images/Background_error.png" />
            </Setter.Value>
        </Setter>
        <Setter Property="StackPanel.Margin" Value="0 604 0 0" />
        <Setter Property="StackPanel.Clip">
            <Setter.Value>
                <RectangleGeometry RadiusX="36" RadiusY="36" Rect="0,0,960,713" />
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="ErrorButtons">
        <Setter Property="StackPanel.Orientation" Value="Horizontal" />
        <Setter Property="StackPanel.Width" Value="720" />
        <Setter Property="StackPanel.Margin" Value="0 95 0 0" />
    </Style>

    <Style x:Key="MainStackPanel_Page4">
        <Setter Property="StackPanel.Width" Value="795" />
        <Setter Property="StackPanel.Height" Value="1156" />
        <Setter Property="StackPanel.HorizontalAlignment" Value="Center" />
        <Setter Property="StackPanel.Orientation" Value="Vertical" />
    </Style>

    <Style x:Key="Main22" BasedOn="{StaticResource Main2}">
        <Setter Property="StackPanel.Margin" Value="0 40 0 0" />
    </Style>

</ResourceDictionary>