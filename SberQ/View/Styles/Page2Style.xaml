<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Style x:Key="MainStackPanel1">
        <Setter Property="StackPanel.Orientation" Value="Vertical" />
        <Setter Property="StackPanel.VerticalAlignment" Value="Top"/>
        <Setter Property="StackPanel.Width" Value="960"/>
        <Setter Property="StackPanel.Margin" Value="60 169 60 0"/>
    </Style>

    <Style x:Key="Button_Cancel">
        <Setter Property="Button.Background" Value="#33FFFFFF" />
        <Setter Property="Button.Width" Value="223" />
        <Setter Property="Button.Height" Value="62" />
        <Setter Property="Button.Margin" Value="0 60 60 0" />
        <Setter Property="Button.FontFamily" Value="pack://application:,,,/SberQ;component/Assets/Fonts/#SB Sans Text"/>
        <Setter Property="Button.FontWeight" Value="Regular"/>
        <Setter Property="Button.FontSize" Value="24" />
        <Setter Property="Button.Foreground" Value="#FFFFFF" />
        <Setter Property="Button.BorderThickness" Value="1" />
        <Setter Property="Button.BorderBrush" Value="#FFFFFFFF" />
        <Setter Property="Button.Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border x:Name="border" Background="{TemplateBinding Background}" BorderThickness="1" BorderBrush="#FFFFFF" CornerRadius="12">
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="border" Property="Background" Value="#45FFfFFF" />
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="TextSubjectTestSberQ">
        <Setter Property="TextBlock.FontWeight" Value="SemiBold"/>
        <Setter Property="TextBlock.FontFamily" Value="pack://application:,,,/SberQ;component/Assets/Fonts/#SB Sans Display"/>
        <Setter Property="TextBlock.Foreground" Value="White"/>
        <Setter Property="TextBlock.FontSize" Value="72"/>
        <Setter Property="TextBlock.TextAlignment" Value="Left"/>
    </Style>

    

    <Style x:Key="TextBody1TestSberQ">
        <Setter Property="TextBlock.FontWeight" Value="Regular"/>
        <Setter Property="TextBlock.FontFamily" Value="pack://application:,,,/SberQ;component/Assets/Fonts/#SB Sans Text"/>
        <Setter Property="TextBlock.Foreground" Value="White"/>
        <Setter Property="TextBlock.TextWrapping" Value="Wrap"/>
        <Setter Property="TextBlock.Margin" Value="0 42 0 0"/>
        <Setter Property="TextBlock.TextAlignment" Value="Left"/>
        <Setter Property="TextBlock.FontSize" Value="24"/>
        <Setter Property="TextBlock.Width" Value="auto" />
    </Style>
    
    <Style x:Key="TextBody2TestSberQ" BasedOn="{StaticResource TextBody1TestSberQ}">
        <Setter Property="TextBlock.Margin" Value="0 16 0 0"/>
    </Style>

    <Style x:Key="MainStackPanel11">
        <Setter Property="StackPanel.Margin" Value="0 95 0 0"/>
        <Setter Property="StackPanel.Clip">
        <Setter.Value>
            <RectangleGeometry Rect="0,0,960,1158" RadiusX="36" RadiusY="36"/>
        </Setter.Value>
        </Setter>
        <Setter Property="StackPanel.Orientation" Value="Vertical" />
        <Setter Property="StackPanel.HorizontalAlignment" Value="Right"/>
        <Setter Property="StackPanel.VerticalAlignment" Value="Center"/>
        <Setter Property="StackPanel.Margin" Value="0 42 0 0"/>
        <Setter Property="StackPanel.Background" Value="#33FFFFFF"/>
        <Setter Property="StackPanel.Height" Value="1158"/>
        <Setter Property="StackPanel.Width" Value="960"/>
    </Style>
    
    <Style x:Key="TextQuestionTestSberQ">
        <Setter Property="TextBlock.FontWeight" Value="SemiBold"/>
        <Setter Property="TextBlock.FontFamily" Value="pack://application:,,,/SberQ;component/Assets/Fonts/#SB Sans Display"/>
        <Setter Property="TextBlock.Foreground" Value="White"/>
        <Setter Property="TextBlock.FontSize" Value="28"/>
        <Setter Property="TextBlock.TextAlignment" Value="Left"/>
    </Style>

    <Style x:Key="TextQuestionTestSberQMargin07200" BasedOn="{StaticResource TextQuestionTestSberQ}">
        <Setter Property="TextBlock.Margin" Value="0 72 0 0" />
    </Style>

    <Style TargetType="Slider" x:Key="Slider1">
        <Setter Property="Slider.Value" Value="0"/>
        <Setter Property="Slider.Minimum" Value="0"/>
        <Setter Property="Slider.Maximum" Value="2"/>
        <Setter Property="Slider.SelectionStart" Value="0"/>
        <Setter Property="Slider.TickFrequency" Value="1"/>
        <Setter Property="Slider.IsSnapToTickEnabled" Value="True"/>
        <Setter Property="Slider.IsSelectionRangeEnabled" Value="True"/>
        <Setter Property="Slider.Width" Value="880"/>
        <Setter Property="Slider.Margin" Value="0 24 0 0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Slider">
                    <Grid Height="51" VerticalAlignment="Center">
                        <Track x:Name="PART_Track"
                                Minimum="{TemplateBinding Minimum}"
                                Maximum="{TemplateBinding Maximum}"
                                Value="{TemplateBinding Value}">


                            <Track.DecreaseRepeatButton>
                                <RepeatButton  Command="Slider.DecreaseLarge"
                                               Background="#FFFFFF"
                                               IsTabStop="False"
                                               Height="32"
                                               HorizontalAlignment="Stretch"
                                               MinWidth="0"
                                               BorderThickness="0"
                                               IsHitTestVisible="False"
                                               Width="auto"
                                               Margin="0,0,-24,0">
                                    <RepeatButton.Clip>
                                        <RectangleGeometry 
                                            RadiusX="20" 
                                            RadiusY="20" 
                                            Rect="0 0 880 32"/>
                                    </RepeatButton.Clip>
                                </RepeatButton>
                            </Track.DecreaseRepeatButton>


                            <Track.IncreaseRepeatButton>
                                <RepeatButton Command="Slider.IncreaseLarge"
                                              Background="#33FFFFFF"
                                              IsTabStop="False"
                                              Height="32"
                                              HorizontalAlignment="Stretch"
                                              MinWidth="0"
                                              Width="auto"
                                              BorderThickness="0"
                                              IsHitTestVisible="False"
                                              Margin="-27,0,0,0">
                                </RepeatButton>
                            </Track.IncreaseRepeatButton>

                            <Track.Thumb>
                                <Thumb Width="51" Height="51" BorderThickness="0" Margin="-9 2 0 0">
                                    <Thumb.Template>
                                        <ControlTemplate TargetType="Thumb">
                                            <Ellipse>
                                                <Ellipse.Fill>
                                                    <ImageBrush ImageSource="/Assets/Images/Group34.png"/>
                                                </Ellipse.Fill>
                                            </Ellipse>
                                        </ControlTemplate>
                                    </Thumb.Template>
                                    <Thumb.Clip>
                                        <EllipseGeometry Center="25.5,25.5" RadiusX="20" RadiusY="20" />
                                    </Thumb.Clip>
                                </Thumb>
                            </Track.Thumb>
                        </Track>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="GridTestSberQ">
        <Setter Property="Grid.Margin" Value="0 8 0 0" />
    </Style>

    <Style x:Key="GridTextTestSberQ">
        <Setter Property="TextBlock.FontFamily" Value="pack://application:,,,/SberQ;component/Assets/Fonts/#SB Sans Text"/>
        <Setter Property="TextBlock.FontSize" Value="20" />
        <Setter Property="TextBlock.Foreground" Value="White" />
        <Setter Property="TextBlock.FontWeight" Value="Regular"/>
    </Style>

    <Style x:Key="GridTextTestSberQLeft" BasedOn="{StaticResource GridTextTestSberQ}">
        <Setter Property="TextBlock.HorizontalAlignment" Value="Left" />
    </Style>

    <Style x:Key="GridTextTestSberQCenter" BasedOn="{StaticResource GridTextTestSberQ}">
        <Setter Property="TextBlock.HorizontalAlignment" Value="Center" />
    </Style>

    <Style x:Key="GridTextTestSberQRight" BasedOn="{StaticResource GridTextTestSberQ}">
        <Setter Property="TextBlock.HorizontalAlignment" Value="Right" />
    </Style>

    <Style x:Key="ButtonSendTestSberQ">
        <Setter Property="Button.Background" Value="White" />
        <Setter Property="Button.FontFamily" Value="pack://application:,,,/SberQ;component/Assets/Fonts/#SB Sans Text"/>
        <Setter Property="Button.FontSize" Value="36" />
        <Setter Property="Button.Foreground" Value="#18AA7B" />
        <Setter Property="Button.BorderThickness" Value="0" />
        <Setter Property="Button.Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border x:Name="border" Background="{TemplateBinding Background}" CornerRadius="24" >
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="border" Property="Background" Value="#E0E0E0"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Button.Width" Value="445" />
        <Setter Property="Button.Height" Value="109" />
        <Setter Property="Button.Margin" Value="0 42 0 0" />
        <Setter Property="Button.FontWeight" Value="Regular"/>
        <Setter Property="Button.HorizontalAlignment" Value="Left" />
    </Style>

</ResourceDictionary>