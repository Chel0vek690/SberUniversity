<Page x:Class="SberQ.Page4"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SberQ"
      mc:Ignorable="d" 
      d:DesignHeight="1920" d:DesignWidth="1080"
      Title="Page4">
    <Grid>
        <StackPanel Style="{StaticResource MainStackPanel_Page4}">
            <TextBlock Style="{StaticResource YourResults}">Ваши результаты</TextBlock>
            <TextBlock Style="{StaticResource YourResults}">успешно отправлены!</TextBlock>
            <TextBlock Style="{StaticResource Body_Page4}">Спасибо за прохождение теста</TextBlock>
            <StackPanel Style="{StaticResource Main2}">
                <StackPanel Style="{StaticResource Main3}">
                    <Image Style="{StaticResource QRCode2}"/>
                    <TextBlock Style="{StaticResource TextQR1}" Text="Узнать больше&#xA;про оценку руководителей SberQ"></TextBlock>
                </StackPanel>
            </StackPanel>

            <StackPanel Style="{StaticResource Main22}">
                <StackPanel Style="{StaticResource Main3}">
                    <Image Style="{StaticResource QRCode3}" />
                    <TextBlock Style="{StaticResource TextQR2}" Text="Узнать&#xA;об образовательных&#xA;решениях&#xA;СберУниверситета"></TextBlock>
                </StackPanel>
            </StackPanel>
            <Button Command="{Binding NavigateStartCommand}" Style="{StaticResource ButtonStart}">На главную</Button>
        </StackPanel>
    </Grid>
</Page>